# Supabase Configuration
# Get these from your Supabase project settings -> API
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE=your-service-role-key-here
SUPABASE_ANON=your-anon-key-here

# Trading Configuration (defaults provided)
FEE_BPS_DEFAULT=60
SPREAD_DEFAULT=0.01
SLIPPAGE_DEFAULT=0.005

# Astro Scoring Parameters
LAMBDA_GAIN=0.10
EDGE_THRESHOLD=0.04
LAMBDA_DAYS=5
ORB_LIMITS_JSON={"square": 8, "opposition": 8, "conjunction": 6}
K_CAP=5.0

# Application Settings
APP_VERSION=0.1.0
DEBUG=true

# Telegram Bot (optional for bot service)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here

# Development settings
NODE_ENV=development