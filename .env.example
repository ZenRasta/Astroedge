# Supabase Configuration
# Get these from your Supabase project settings -> API
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE=your-service-role-key-here
SUPABASE_ANON=your-anon-key-here

# Trading Configuration (defaults provided)
FEE_BPS_DEFAULT=60
SPREAD_DEFAULT=0.01
SLIPPAGE_DEFAULT=0.005

# Astro Scoring Parameters
LAMBDA_GAIN=0.10
EDGE_THRESHOLD=0.04
LAMBDA_DAYS=5
ORB_LIMITS_JSON={"square": 8, "opposition": 8, "conjunction": 6}
K_CAP=5.0

# Application Settings
APP_VERSION=0.1.0
DEBUG=true

# Polymarket Integration
POLY_BASE_URL=https://gamma-api.polymarket.com
CLOB_BASE_URL=https://clob.polymarket.com
POLY_TIMEOUT_S=10
REDIS_URL=redis://redis:6379/0

# LLM Configuration
LLM_MODEL=openai/gpt-4o-mini
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Caching Configuration  
TAG_CACHE_TTL_SEC=604800           # 7 days for LLM tags
ORDERBOOK_CACHE_TTL_SEC=3          # 3s micro-cache for /books

# Liquidity Configuration
LIQUIDITY_SPREAD_WIDE=0.05         # 5% considered wide spread
LIQUIDITY_DEPTH_MAX_USDC=5000      # Max depth for normalization
LIQUIDITY_MIN_SCORE=0.50           # Minimum liquidity score threshold

# Concurrency Configuration
SCAN_CONCURRENCY=16                # Concurrent market scanning
BOOKS_BATCH=40                     # Token IDs per CLOB /books batch

# Telegram Bot (optional for bot service)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here

# Development settings
NODE_ENV=development